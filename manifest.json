    "update_url": "https://gitlab.com/magnolia1234/bypass-paywalls-chrome-clean/-/raw/master/updates.xml",
    "short_name": "Bypass Paywall",
    "options_ui": {
        "chrome_style": true,
        "open_in_tab": true,
        "page": "options/options.html"
    },
    "incognito": "split",
    "optional_permissions": [
        "<all_urls>"
    ],
